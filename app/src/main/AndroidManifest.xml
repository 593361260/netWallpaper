<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gaqiujun.moment1">

    <application
        android:name=".core.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/Theme.Wallper">
        <activity android:name="com.gaqiujun.moment1.module.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".module.details.PicDetailsActivity" />
        <activity android:name=".module.subjectDetails.SubjectDetailsActivity" />
    </application>
    <uses-permission android:name="android.permission.INTERNET" />
</manifest>